// æ¤œç´¢æ©Ÿèƒ½
function searchEmojis(inputText) {
    if (!inputText.trim()) {
        return '';
    }
    
    // ã‚¹ãƒšãƒ¼ã‚¹ã§åŒºåˆ‡ã£ã¦ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰ã‚’åˆ†å‰²
    const keywords = inputText.trim().split(/\s+/);
    
    // å„ã‚­ãƒ¼ãƒ¯ãƒ¼ãƒ‰ã«å¯¾å¿œã™ã‚‹çµµæ–‡å­—ã‚’æ¤œç´¢
    const emojis = keywords.map(keyword => {
        const normalizedKeyword = keyword.toLowerCase();
        return emojiDictionary[normalizedKeyword] || fallbackEmoji;
    });
    
    return emojis.join(' ');
}

// æ¤œç´¢å®Ÿè¡Œã¨UIæ›´æ–°
function performSearch() {
    const inputText = searchInput.value;
    
    if (!inputText.trim()) {
        resultDisplay.textContent = '';
        hideActionButtons();
        return;
    }
    
    // ãƒ­ãƒ¼ãƒ‡ã‚£ãƒ³ã‚°è¡¨ç¤º
    resultDisplay.textContent = 'æ¤œç´¢ä¸­...';
    resultDisplay.className = 'result-display loading';
    hideActionButtons();
    
    // å°‘ã—é…å»¶ã‚’å…¥ã‚Œã¦ãƒ­ãƒ¼ãƒ‡ã‚£ãƒ³ã‚°æ„Ÿã‚’æ¼”å‡º
    setTimeout(() => {
        const result = searchEmojis(inputText);
        resultDisplay.textContent = result;
        resultDisplay.className = 'result-display';
        
        // çµæœãŒã‚ã‚‹å ´åˆã¯ã‚¢ã‚¯ã‚·ãƒ§ãƒ³ãƒœã‚¿ãƒ³ã‚’è¡¨ç¤º
        if (result) {
            showActionButtons();
        }
    }, 300);
}

// ã‚¤ãƒ™ãƒ³ãƒˆãƒªã‚¹ãƒŠãƒ¼ã®è¨­å®š
searchButton.addEventListener('click', performSearch);

// ã‚¨ãƒ³ã‚¿ãƒ¼ã‚­ãƒ¼ã§ã®æ¤œç´¢
searchInput.addEventListener('keypress', function(event) {
    if (event.key === 'Enter') {
        performSearch();
    }
});

// å…¥åŠ›ãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰ã«ãƒ•ã‚©ãƒ¼ã‚«ã‚¹ã‚’å½“ã¦ã‚‹
