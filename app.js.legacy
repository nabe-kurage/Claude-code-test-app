    }, 3000);
}

function hideNotification() {
    notification.classList.remove('show');
    setTimeout(() => {
        notification.style.display = 'none';
    }, 300);
}

// ã‚³ãƒ”ãƒ¼æ©Ÿèƒ½
async function copyToClipboard() {
    const text = resultDisplay.textContent;
    
    if (!text || text === 'æ¤œç´¢ä¸­...') {
        showNotification('ã‚³ãƒ”ãƒ¼ã§ãã‚‹çµµæ–‡å­—ãŒã‚ã‚Šã¾ã›ã‚“', 'error');
        return;
    }
    
    try {
        // Clipboard API ã‚’è©¦è¡Œ
        if (navigator.clipboard && window.isSecureContext) {
            await navigator.clipboard.writeText(text);
            showNotification('çµµæ–‡å­—ã‚’ã‚³ãƒ”ãƒ¼ã—ã¾ã—ãŸ! ğŸ“‹');
            
            // ãƒœã‚¿ãƒ³ã‚¢ãƒ‹ãƒ¡ãƒ¼ã‚·ãƒ§ãƒ³
            copyButton.classList.add('clicked');
            setTimeout(() => {
                copyButton.classList.remove('clicked');
            }, 300);
            
            // ä¸€æ™‚çš„ã«ãƒœã‚¿ãƒ³ãƒ†ã‚­ã‚¹ãƒˆã‚’å¤‰æ›´
            const originalText = copyButton.innerHTML;
            copyButton.innerHTML = 'âœ… ã‚³ãƒ”ãƒ¼æ¸ˆã¿';
            setTimeout(() => {
                copyButton.innerHTML = originalText;
            }, 2000);
            
        } else {
            // ãƒ•ã‚©ãƒ¼ãƒ«ãƒãƒƒã‚¯: å¤ã„ãƒ–ãƒ©ã‚¦ã‚¶å¯¾å¿œ
            const textArea = document.createElement('textarea');
            textArea.value = text;
            document.body.appendChild(textArea);
            textArea.select();
            const successful = document.execCommand('copy');
            document.body.removeChild(textArea);
            
            if (successful) {
                showNotification('çµµæ–‡å­—ã‚’ã‚³ãƒ”ãƒ¼ã—ã¾ã—ãŸ! ğŸ“‹');
            } else {
                throw new Error('ã‚³ãƒ”ãƒ¼ã«å¤±æ•—ã—ã¾ã—ãŸ');
            }
        }
    } catch (error) {
        console.error('ã‚³ãƒ”ãƒ¼ã‚¨ãƒ©ãƒ¼:', error);
        showNotification('ã‚³ãƒ”ãƒ¼ã«å¤±æ•—ã—ã¾ã—ãŸ', 'error');
    }
}

// ã‚·ã‚§ã‚¢æ©Ÿèƒ½
async function shareContent() {
    const emojis = resultDisplay.textContent;
    const inputText = searchInput.value;
    
    if (!emojis || emojis === 'æ¤œç´¢ä¸­...') {
        showNotification('ã‚·ã‚§ã‚¢ã§ãã‚‹çµµæ–‡å­—ãŒã‚ã‚Šã¾ã›ã‚“', 'error');
        return;
    }
    
    const shareText = `ã€Œ${inputText}ã€â†’ ${emojis}`;
    const shareUrl = window.location.href;
    
    try {
        // Web Share API ã‚’è©¦è¡Œï¼ˆãƒ¢ãƒã‚¤ãƒ«å¯¾å¿œï¼‰
        if (navigator.share) {
            await navigator.share({
                title: 'çµµæ–‡å­—æ¤œç´¢ã‚¢ãƒ—ãƒª',
                text: shareText,
                url: shareUrl
            });
            showNotification('ã‚·ã‚§ã‚¢ã—ã¾ã—ãŸ! ğŸ“¤');
            return;
        }
    } catch (error) {
        console.log('Web Share API ã‚¨ãƒ©ãƒ¼:', error);
    }
    
    // ãƒ•ã‚©ãƒ¼ãƒ«ãƒãƒƒã‚¯: SNSå€‹åˆ¥ã‚·ã‚§ã‚¢
    showShareMenu();
}

// ã‚·ã‚§ã‚¢ãƒ¡ãƒ‹ãƒ¥ãƒ¼ã®è¡¨ç¤º
function showShareMenu() {
    const emojis = resultDisplay.textContent;
    const inputText = searchInput.value;
    const shareText = `ã€Œ${inputText}ã€â†’ ${emojis}`;
    const shareUrl = window.location.href;
    
    // ã‚·ã‚§ã‚¢ãƒ¡ãƒ‹ãƒ¥ãƒ¼ãŒã™ã§ã«å­˜åœ¨ã™ã‚‹å ´åˆã¯å‰Šé™¤
    const existingMenu = document.querySelector('.share-menu');
    if (existingMenu) {
        existingMenu.remove();
    }
    
    // ã‚·ã‚§ã‚¢ãƒ¡ãƒ‹ãƒ¥ãƒ¼ã‚’ä½œæˆ
    const shareMenu = document.createElement('div');
    shareMenu.className = 'share-menu';
    shareMenu.innerHTML = `
        <a href="https://twitter.com/intent/tweet?text=${encodeURIComponent(shareText)}&url=${encodeURIComponent(shareUrl)}" target="_blank" class="share-option">
            ğŸ¦ Twitter ã§ã‚·ã‚§ã‚¢
        </a>
        <a href="https://social-plugins.line.me/lineit/share?url=${encodeURIComponent(shareUrl)}&text=${encodeURIComponent(shareText)}" target="_blank" class="share-option">
            ğŸ’š LINE ã§ã‚·ã‚§ã‚¢
        </a>
        <button class="share-option" onclick="copyShareText(\`${shareText.replace(/`/g, '\\`')}\`)">
            ğŸ“‹ ãƒ†ã‚­ã‚¹ãƒˆã‚’ã‚³ãƒ”ãƒ¼
        </button>
    `;
    
    // ã‚·ã‚§ã‚¢ãƒœã‚¿ãƒ³ã®ä½ç½®ã«è¿½åŠ 
    shareButton.style.position = 'relative';
    shareButton.appendChild(shareMenu);
    
    // è¡¨ç¤ºã‚¢ãƒ‹ãƒ¡ãƒ¼ã‚·ãƒ§ãƒ³
    setTimeout(() => {
        shareMenu.classList.add('visible');
    }, 10);
    
    // 3ç§’å¾Œã«è‡ªå‹•ã§éè¡¨ç¤º
    setTimeout(() => {
        if (shareMenu.parentNode) {
            shareMenu.remove();
        }
    }, 5000);
    
    // ä»–ã®å ´æ‰€ã‚’ã‚¯ãƒªãƒƒã‚¯ã—ãŸæ™‚ã«éè¡¨ç¤º
    document.addEventListener('click', function hideMenu(event) {
        if (!shareButton.contains(event.target)) {
            if (shareMenu.parentNode) {
                shareMenu.remove();
            }
            document.removeEventListener('click', hideMenu);
        }
    });
}

// ã‚·ã‚§ã‚¢ãƒ†ã‚­ã‚¹ãƒˆã®ã‚³ãƒ”ãƒ¼
async function copyShareText(text) {
    try {
        if (navigator.clipboard && window.isSecureContext) {
            await navigator.clipboard.writeText(text);
        } else {
            const textArea = document.createElement('textarea');
            textArea.value = text;
            document.body.appendChild(textArea);
            textArea.select();
            document.execCommand('copy');
            document.body.removeChild(textArea);
        }
        showNotification('ã‚·ã‚§ã‚¢ç”¨ãƒ†ã‚­ã‚¹ãƒˆã‚’ã‚³ãƒ”ãƒ¼ã—ã¾ã—ãŸ! ğŸ“‹');
    } catch (error) {
        showNotification('ã‚³ãƒ”ãƒ¼ã«å¤±æ•—ã—ã¾ã—ãŸ', 'error');
    }
}

