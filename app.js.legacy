// ã‚¢ã‚¯ã‚·ãƒ§ãƒ³ãƒœã‚¿ãƒ³ã®è¡¨ç¤º/éè¡¨ç¤º
function showActionButtons() {
    actionButtons.style.display = 'flex';
    setTimeout(() => {
        actionButtons.classList.add('visible');
